         {this.state.books &&\n                this.state.books.map((book) => \n                <tr className=\"row\">\n                    <td key={book.bookID}>{book.bookID}</td>\n                    <td>{book.bookName}</td>\n                    <td>{book.firstAuthor}</td>\n                    <td>{book.secondAuthor}</td>\n                    <td>{book.totalAmount}</td>\n                    <td>{book.leftAmount}</td>\n                    <td><button className=\"btn-reserve\" onClick={this.reserve}>Reserve</button></td>\n                </tr>\n                )}\n            </tbody> \n            </table>\n            </div>)\n        return booklist\n    }\n\n    render(){\n        return(\n            <div className='App'>\n                {this.tableMethod()\n                }\n            </div>\n        )\n    }\n}\n\nexport default FirebaseData;","/home/fattijenishbek/react-website/src/components/Firebase.js",[],"/home/fattijenishbek/react-website/src/Login.js",["68","69","70"],"/home/fattijenishbek/react-website/src/Log.js",[],{"ruleId":"71","severity":1,"message":"72","line":3,"column":34,"nodeType":"73","messageId":"74","endLine":3,"endColumn":40},{"ruleId":"71","severity":1,"message":"75","line":3,"column":42,"nodeType":"73","messageId":"74","endLine":3,"endColumn":47},{"ruleId":"71","severity":1,"message":"76","line":7,"column":8,"nodeType":"73","messageId":"74","endLine":7,"endColumn":19},{"ruleId":"71","severity":1,"message":"77","line":9,"column":12,"nodeType":"73","messageId":"74","endLine":9,"endColumn":18},{"ruleId":"78","severity":1,"message":"79","line":34,"column":53,"nodeType":"80","endLine":34,"endColumn":70},{"ruleId":"81","severity":1,"message":"82","line":35,"column":77,"nodeType":"83","endLine":35,"endColumn":124},{"ruleId":"78","severity":1,"message":"79","line":66,"column":62,"nodeType":"80","endLine":66,"endColumn":79},{"ruleId":"78","severity":1,"message":"79","line":71,"column":112,"nodeType":"80","endLine":71,"endColumn":129},{"rule