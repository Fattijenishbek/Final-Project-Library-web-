li className='nav-item'>\n                            <Link to='/' className='nav-links' onClick={closeMobileMenu}>\n                                Home\n                            </Link>\n                        </li>\n                        <li className='nav-item'>\n                            <Link to='/pictures' className='nav-links' onClick={closeMobileMenu}>\n                                Pictures\n                            </Link>\n                        </li>\n                        <li className='nav-item'>\n                            <Link to='/army' className='nav-links' onClick={closeMobileMenu}>\n                                Army\n                            </Link>\n                        </li>\n                        {/* <li className='nav-item'>\n                            <Link to='/bighit' className='nav-links' onClick={closeMobileMenu}>\n                                BigHit\n                            </Link>\n                        </li> */}\n                        <li className='nav-item'>\n                            <Link to='/sign-up' className='nav-links-mobile' onClick={closeMobileMenu}>\n                                Sign Up\n                            </Link>\n                        </li>\n                    </ul>\n                    {button && <Button buttonStyle='btn--outline'>SIGN UP</Button>}\n                </div>\n            </nav>\n        </div>\n    )\n}\n\nexport default Navbar","/home/fattijenishbek/react-website/src/components/Button.js",[],"/home/fattijenishbek/react-website/src/components/HeroSection.js",["46"],"/home/fattijenishbek/react-website/src/components/pages/Home.js",[],"/home/fattijenishbek/react-website/src/components/Footer.js",["47"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport { Button } from './Button';\nimport './Footer.css'\nimport bts_logo from '../bts_logo.png';\n\n\nfunction Footer (){\n    return (\n        <div className='footer-container'>